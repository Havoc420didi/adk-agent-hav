# ADK 自定义应用环境配置示例
# 复制此文件为 .env 并修改相应配置

# 应用配置
APP_NAME=my_custom_app
DEBUG=true

# Web 接口配置
# 设置为 true 使用原始 ADK Web UI（需要完整的 Angular 前端）
# 设置为 false 使用自定义 Web 接口（简化版聊天界面）
USE_ORIGINAL_ADK_WEB=false

# DeepSeek API 配置
MODEL_NAME=deepseek/deepseek-chat
API_KEY=your_api_key_here
API_BASE=https://api.deepseek.com

# 服务器配置
HOST=127.0.0.1
PORT=8000

# 可选配置
# USE_IN_MEMORY=true
# SESSION_SERVICE_URI=
# GCS_BUCKET=
# MAX_TOKENS=2000
# TEMPERATURE=0.7

# 使用说明:
# 1. 复制此文件为 .env
# 2. 设置您的 API_KEY
# 3. 根据需要调整其他配置
# 4. 运行: python main.py --mode web 